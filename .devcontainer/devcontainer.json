{
    "name": "RustPacker",
    // image being used
     "image": "mcr.microsoft.com/devcontainers/rust:latest",
     // install dependencies
     "updateContentCommand": "rustup target add x86_64-pc-windows-gnu",
     "customizations": {
         // install some vscode extensions
         "vscode": {
             "extensions": [
                 "rust-lang.rust-analyzer"
             ]
         }
     }
  }